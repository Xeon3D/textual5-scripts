on textualcmd(cmd)--set cmd to "textual@187-177-41-69.dynamic.axtel.net"set ipaddr to cmdif cmd contains "@" then	set AppleScript's text item delimiters to "@"	set ipaddr to cmd's text item 2	set AppleScript's text item delimiters to "."	set ipaddr to ipaddr's text item 1	set AppleScript's text item delimiters to "-"	set ipaddr to words of ipaddr as list	set AppleScript's text item delimiters to "."	set ipaddr to items of ipaddr as textend ifset ipaddr to cmdset results to do shell script "curl http://ip-api.com/csv/" & ipaddrset AppleScript's text item delimiters to ","if results starts with "fail" then	return "Error."end iftry	set country to do shell script "echo " & results's text item 2 & " | tr -d '\"'"on error	set country to results's text item 2end tryset state to results's text item 4try	set city to do shell script "echo " & results's text item 6 & " | tr -d '\"'"on error	set city to results's text item 6end trytry	set isp to do shell script "echo " & results's text item 11 & " | tr -d '\"'"on error	set isp to results's text item 11end tryset msg to "The IP Address " & ipaddr & " is from " & city & ", " & state & ", " & country & ". The ISP is:" & isp
set msg2 to "The command typed was: /dnsinfo " & cmdreturn "/debug " & msgend